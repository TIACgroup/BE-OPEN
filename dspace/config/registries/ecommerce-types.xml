        <contributor.author>Andrea Bollini</contributor.author>
        <contributor.author>Luigi Andrea Pascarelli</contributor.author>
        <contributor.author>Andrea Petrucci</contributor.author>
    </dspace-header>

    <dc-schema>
        <name>ec</name>
        <namespace>http://dspace.org/ecommerce</namespace>
