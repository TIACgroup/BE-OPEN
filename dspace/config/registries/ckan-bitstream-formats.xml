<dspace-bitstream-types>

  <bitstream-type>
    <mimetype>text/csv</mimetype>
    <short_description>Comma Separated Values</short_description>
    <description>Tabular data in plain text file with comma delimitator (CSV)</description>
    <support_level>1</support_level>
    <internal>false</internal>
    <extension>csv</extension>
  </bitstream-type>

   <bitstream-type>
    <mimetype>text/tab-separated-values</mimetype>
    <short_description>Tab Separated Values</short_description>
    <description>Tabular data in plain text file with comma delimitator (TSV)</description>
    <support_level>1</support_level>
    <internal>false</internal>
    <extension>tsv</extension>
  </bitstream-type>


</dspace-bitstream-types>
